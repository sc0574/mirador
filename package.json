    "redux-thunk": "^3.1.0",
    "reselect": "^5.0.0",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "url": "^0.11.0",
    "uuid": "^8.1.0 || ^9.0.0 || ^10.0.0 || ^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
